{
  "name": "auto-document",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.2",
  "workspaces": [
    "apps/*",
    "packages/*",
    "proto/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "debug": "turbo run debug",
    "generate:ci": "node scripts/generate-gitlab-ci.js",
    "proto:generate": "protoc --ts_proto_out=./packages/types/src/ ./proto/*.proto --ts_proto_opt=nestJs=true"
  },
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dotenv": "^8.2.3",
    "ts-proto": "^2.8.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.9",
    "rxjs": "^7.8.2",
    "turbo": "^2.6.3"
  }
}
