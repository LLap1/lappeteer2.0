{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "public/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "debug": {
      "persistent": true,
      "cache": false
    }
  }
}
